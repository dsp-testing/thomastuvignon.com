{
  "plugins": {
    "postcss-preset-env": {
      "stage": 0,
      "features": {
        "color-functional-notation": false,
        "custom-properties": false,
        "gap-properties": false,
        "lab-function": { "preserve": true },
        "logical-properties-and-values": false,
        "unset-value": false
      },
      "importFrom": ["./styles/variables.css"]
    }
  }
}
